{
    "name": "sf4/api-security",
    "type": "library",
    "description": "Simple security package for Symfony 4",
    "keywords": [
        "sf4",
        "api-security"
    ],
    "homepage": "https://github.com/sf4/api-security",
    "license": "MIT",
    "authors": [
        {
            "name": "Siim Liimand",
            "email": "siim.liimand@gmail.com",
            "homepage": "https://github.com/siimliimand",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.1",
        "sf4/api-user": "^1.3",
        "symfony/security-bundle": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Sf4\\ApiSecurity\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sf4\\ApiSecurity\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
